        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Prepare Query Params",
          "script": "import sys\nimport urllib.parse\n\n(id, limit) = sys.argv[1:3]\n\nqueryStr = \"\"\nfirst = True\n\nif id != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"id=\" + urllib.parse.quote_plus(str(id))\n    first = False\n\nif limit != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"limit=\" + urllib.parse.quote_plus(str(limit))\n    first = False\n\nprint(queryStr)\n",
          "script_arguments": [
