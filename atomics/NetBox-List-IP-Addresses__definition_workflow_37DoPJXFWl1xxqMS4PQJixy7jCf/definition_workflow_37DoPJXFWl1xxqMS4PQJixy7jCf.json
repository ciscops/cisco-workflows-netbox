        "unique_name": "definition_activity_37DoPD3kbV82pVMhEykAdi5dCOQ",
        "name": "Execute Python Script",
        "title": "Prepare Query Params",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Prepare Query Params",
          "script": "import sys\nimport urllib.parse\n\n(description, dns_name, id, limit, q, status, tenant) = sys.argv[1:8]\n\nqueryStr = \"\"\nfirst = True\n\nif description != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"description=\" + urllib.parse.quote_plus(str(description))\n    first = False\n\nif dns_name != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"dns_name=\" + urllib.parse.quote_plus(str(dns_name))\n    first = False\n\nif id != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"id=\" + urllib.parse.quote_plus(str(id))\n    first = False\n\nif limit != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"limit=\" + urllib.parse.quote_plus(str(limit))\n    first = False\n\nif q != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"q=\" + urllib.parse.quote_plus(str(q))\n    first = False\n\nif status != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"status=\" + urllib.parse.quote_plus(str(status))\n    first = False\n\nif tenant != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"tenant=\" + urllib.parse.quote_plus(str(tenant))\n    first = False\n\nprint(queryStr)\n",
          "script_arguments": [
