        "unique_name": "definition_activity_37G968WAbqQRqg3Na4z7JBEHNiH",
        "name": "Execute Python Script",
        "title": "Prepare Query Params",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Prepare Query Params",
          "script": "import sys\nimport urllib.parse\n\n(description, has_primary_ip, id, limit, name, q, site, status, tenant) = sys.argv[1:10]\n\nqueryStr = \"\"\nfirst = True\n\nif description != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"description=\" + urllib.parse.quote_plus(str(description))\n    first = False\n\nif has_primary_ip != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"has_primary_ip=\" + urllib.parse.quote_plus(str(has_primary_ip))\n    first = False\n\nif id != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"id=\" + urllib.parse.quote_plus(str(id))\n    first = False\n\nif limit != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"limit=\" + urllib.parse.quote_plus(str(limit))\n    first = False\n\nif name != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"name=\" + urllib.parse.quote_plus(str(name))\n    first = False\n\nif q != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"q=\" + urllib.parse.quote_plus(str(q))\n    first = False\n\nif site != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"site=\" + urllib.parse.quote_plus(str(site))\n    first = False\n\nif status != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"status=\" + urllib.parse.quote_plus(str(status))\n    first = False\n\nif tenant != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"tenant=\" + urllib.parse.quote_plus(str(tenant))\n    first = False\n\nprint(queryStr)\n",
          "script_arguments": [
