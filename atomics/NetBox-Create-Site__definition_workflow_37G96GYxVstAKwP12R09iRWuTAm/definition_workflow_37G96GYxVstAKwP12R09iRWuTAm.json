          "continue_on_failure": false,
          "display_name": "Prepare Request Body",
          "script": "import json\n\nasns = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96KPPfgHcOtkwatzlajbsudR$'\ncomments = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96Eh3Z6sCIcHkxNhLfzfM4dB$'\ncustom_fields = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96HM0D0jiHyQY2D89t4Ffsgm$'\ndescription = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96HCq1FqrdGxf89MJ5H0HZNy$'\nfacility = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96K5OKnPKZdVqV4Tp8mlnMaE$'\ngroup = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96IZwv6cDaTbDgxY7hUCXo2j$'\nlatitude = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96HXxgnbw3IWKswGqDU2oT8u$'\nlongitude = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96IgCBUiKhHXm23XjzUKdODg$'\nname = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96JKYYf9nGHaHQQdZyobU9Hu$'\nphysical_address = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96DuSmvlMYCWqbW4E2Y5T4zN$'\nregion = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96KLzYUPGvapHegeJKAqlYJw$'\nshipping_address = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96GVdTlnclPn46R9fDBtMhVK$'\nslug = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96GXuZ7vQnU7qarKOJjjg9Ef$'\nstatus = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96ImN3wDSjWaIAtc7JbXkE7H$'\ntags = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96JcrfipWtYj6UavH00pybtS$'\ntenant = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96H2vwElkb5fD7bc6LVKwzud$'\ntime_zone = '$workflow.definition_workflow_37G96GYxVstAKwP12R09iRWuTAm.input.variable_workflow_37G96JLxj3OeuBxVcKxQg0cyxv4$'\n\nrequest_body_object = {}\nif asns != '':\n    value = json.loads(asns) if asns != '' else None\n    if value is not None:\n        request_body_object[\"asns\"] = value\nif comments != '':\n    request_body_object[\"comments\"] = comments\nif custom_fields != '':\n    value = json.loads(custom_fields) if custom_fields != '' else None\n    if value is not None:\n        request_body_object[\"custom_fields\"] = value\nif description != '':\n    request_body_object[\"description\"] = description\nif facility != '':\n    request_body_object[\"facility\"] = facility\nif group != '':\n    request_body_object[\"group\"] = group\nif latitude != '':\n    value = float(latitude) if latitude != '' else None\n    if value is not None:\n        request_body_object[\"latitude\"] = value\nif longitude != '':\n    value = float(longitude) if longitude != '' else None\n    if value is not None:\n        request_body_object[\"longitude\"] = value\nrequest_body_object[\"name\"] = name\nif physical_address != '':\n    request_body_object[\"physical_address\"] = physical_address\nif region != '':\n    request_body_object[\"region\"] = region\nif shipping_address != '':\n    request_body_object[\"shipping_address\"] = shipping_address\nrequest_body_object[\"slug\"] = slug\nif status != '':\n    request_body_object[\"status\"] = status\nif tags != '':\n    value = json.loads(tags) if tags != '' else None\n    if value is not None:\n        request_body_object[\"tags\"] = value\nif tenant != '':\n    request_body_object[\"tenant\"] = tenant\nif time_zone != '':\n    request_body_object[\"time_zone\"] = time_zone\nrequest_body_string = json.dumps(request_body_object)\n",
