        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Prepare Query Params",
          "script": "import sys\nimport urllib.parse\n\n(description, device_id, id, limit, name, q, tag) = sys.argv[1:8]\n\nqueryStr = \"\"\nfirst = True\n\nif description != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"description=\" + urllib.parse.quote_plus(str(description))\n    first = False\n\nif device_id != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"device_id=\" + urllib.parse.quote_plus(str(device_id))\n    first = False\n\nif id != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"id=\" + urllib.parse.quote_plus(str(id))\n    first = False\n\nif limit != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"limit=\" + urllib.parse.quote_plus(str(limit))\n    first = False\n\nif name != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"name=\" + urllib.parse.quote_plus(str(name))\n    first = False\n\nif q != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"q=\" + urllib.parse.quote_plus(str(q))\n    first = False\n\nif tag != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"tag=\" + urllib.parse.quote_plus(str(tag))\n    first = False\n\nprint(queryStr)\n",
