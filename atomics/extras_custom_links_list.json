        "unique_name": "definition_activity_37G79l1xwPbeCwvoPMgJuCKD6Ks",
        "name": "Execute Python Script",
        "title": "Prepare Query Params",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Prepare Query Params",
          "script": "import sys\nimport urllib.parse\n\n(id, limit, name, q) = sys.argv[1:5]\n\nqueryStr = \"\"\nfirst = True\n\nif id != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"id=\" + urllib.parse.quote_plus(str(id))\n    first = False\n\nif limit != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"limit=\" + urllib.parse.quote_plus(str(limit))\n    first = False\n\nif name != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"name=\" + urllib.parse.quote_plus(str(name))\n    first = False\n\nif q != '':\n    if not first:\n        queryStr += '\u0026'\n    queryStr += \"q=\" + urllib.parse.quote_plus(str(q))\n    first = False\n\nprint(queryStr)\n",
          "script_arguments": [
